{
    "categories": [
        {
            "title": "## 🚀 新功能",
            "labels": ["feature", "enhancement", "feat"]
        },
        {
            "title": "## 🐛 Bug修复",
            "labels": ["fix", "bug"]
        },
        {
            "title": "## 📝 文档",
            "labels": ["documentation", "docs"]
        },
        {
            "title": "## ⚡ 性能优化",
            "labels": ["performance", "perf"]
        },
        {
            "title": "## 🔨 重构",
            "labels": ["refactor"]
        },
        {
            "title": "## 🎨 样式",
            "labels": ["style"]
        },
        {
            "title": "## ✅ 测试",
            "labels": ["test"]
        },
        {
            "title": "## 📦 构建",
            "labels": ["build", "ci"]
        },
        {
            "title": "## 🔧 配置变更",
            "labels": ["chore"]
        }
    ],
    "template": "${{CHANGELOG}}\n\n\\* *本更新日志由 [github-changelog-generator](https://github.com/github-changelog-generator/github-changelog-generator) 自动生成*",
    "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
    "max_tags_to_fetch": 500,
    "max_pull_requests": 500,
    "max_back_track_time": 365,
    "exclude_labels": ["duplicate", "question", "invalid", "wontfix", "discussion"],
    "breaking_labels": ["breaking"],
    "enhancement_labels": ["enhancement", "feature"],
    "bug_labels": ["bug", "fix"],
    "deprecated_labels": ["deprecated"],
    "removed_labels": ["removed"],
    "security_labels": ["security"],
    "issues": false
} 